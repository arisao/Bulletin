sole.log(response);\n                })\n                .catch(error => {\n                    // エラーを処理する（必要に応じて）\n                    console.error(error);\n                });\n\n\n        }\n    }\n}\n</script>\n\n<style\n    src=\"/Users/arisaohta/Documents/workspace-spring-tool-suite-4-4.15.1.RELEASE/Bulletin/web/src/views/BulletinView.css\"></style>"],"mappings":"AA2BA,OAAAA,KAAA;;AAEA;EACAC,KAAA;IACA,MAAAC,KAAA,OAAAC,IAAA;IACA,MAAAC,SAAA,OAAAD,IAAA,CAAAD,KAAA;IACAE,SAAA,CAAAC,OAAA,CAAAH,KAAA,CAAAI,OAAA;IAEA;MACAC,IAAA;MACAC,KAAA,EAAAN,KAAA,CAAAO,WAAA,GAAAC,KAAA;MACAC,MAAA,EAAAP,SAAA,CAAAK,WAAA,GAAAC,KAAA;MACAE,OAAA;MACAC,MAAA;MACAC,KAAA,GACA;QAAAC,IAAA;QAAAC,KAAA;QAAAC,QAAA;MAAA,GACA;QAAAF,IAAA;QAAAC,KAAA;QAAAC,QAAA;MAAA,GACA;QAAAF,IAAA;QAAAC,KAAA;QAAAC,QAAA;MAAA,EACA;MACAC,MAAA;MAAA;MACAC,UAAA;IACA;EACA;EACA;EACAC,QAAA;IACAC,OAAA,CAAAC,GAAA;EACA;EAEAC,OAAA;IACAC,UAAA;MAAAC;IAAA;MACA,KAAAb,OAAA,GAAAc,IAAA,CAAAC,SAAA,CAAAF,MAAA;IACA;IACAG,OAAA;MACA;MACA5B,KAAA,CAAA6B,GAAA,cACAC,IAAA,CAAAC,QAAA;QACA;QACAV,OAAA,CAAAC,GAAA,CAAAS,QAAA;MACA,GACAC,KAAA,CAAAC,KAAA;QACA;QACAZ,OAAA,CAAAY,KAAA,CAAAA,KAAA;MACA;IAGA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}